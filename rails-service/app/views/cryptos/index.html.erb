<p id="notice"><%= notice %></p>

<h1>Cryptos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cryptos.each do |crypto| %>
      <tr>
        <p> Name </p><%= crypto.name %><p> price </p><%= crypto.price %><br />
      </tr>
    <% end %>
  </tbody>
</table>

